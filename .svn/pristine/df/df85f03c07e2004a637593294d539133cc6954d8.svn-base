

.neicont_main_left_cont_main
{
	background-color: #fff;
  border-radius: 5px;
  padding: 7px;
  padding-top: 27px;}


  
.neicont_main_left_cont_main .pages
{  text-align: center;
  padding-top: 19px;
  height: 26px;
  width: 100%;
  line-height: 24px;
}
.neicont_main_left_cont_main .pages li
{  float: left;
  margin-right: 4px;}
.neicont_main_left_cont_main .pages li a
{  background-color: #00a6e3;
  color: #fff;
  display: block;
  padding: 0px 13px;
  border-radius: 5px;
  height: 23px;
  line-height: 23px;}
.neicont_main_left_cont_main .pages li a:hover
{
	background-color:#1ec2ff;
	cursor: pointer;}
.neicont_main_left_cont_main .pages li a:linked{
	background-color:#1ec2ff;
}	
.pages ul
{  margin: auto;
  float: right;
  height: 29px;
  padding-right: 100px;}
  
